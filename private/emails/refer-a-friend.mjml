      <mj-section>
        <mj-divider
          border-color="#6563a4"
          border-width="8px"
          border-style="solid">
        </mj-divider>
        <mj-column
          vertical-align="middle">
          <mj-image
            width="144"
            align="left"
            src="{{ROOT_URL}}img/maven/maven_logo.png"
            href="{{ROOT_URL}}"
            alt="">
          </mj-image>
          <mj-divider
            border-color="#aeb8b8"
            border-width="2px"
            border-style="solid">
          </mj-divider>
          <mj-text
            font-size="14px"
            font-family="Arial, Ubuntu, Helvetica, sans-serif, Helvetica, Arial, sans-serif">
            Hi,
          </mj-text>
          <mj-text
            font-size="14px"
            color="#303939;"
            font-family="Arial, Ubuntu, Helvetica, sans-serif, Helvetica, Arial, sans-serif">
              {{senderFirstName}} is inviting you to join Maven, a social shopping site.
          </mj-text>
          <mj-text
            font-size="16px"
            color="#49d0bf"
            font-family="Arial, Ubuntu, Helvetica, sans-serif, Helvetica, Arial, sans-serif">
              {{referralComments}}
          </mj-text>
